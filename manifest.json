{
    "manifest_version": 3,
    "name": "dimterex extension",
    "action": {
        "default_icon": "icons/32.png",
        "default_popup": "html/popup.html"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
    },
    "description": "Extension ",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "64": "icons/64.png",
        "128": "icons/128.png"
    },
    "permissions": [
        "activeTab",
        "storage",
        "contextMenus",
        "webNavigation",
        "tabs",
        "scripting"
    ],
    "background": {
        "service_worker": "content-scripts/context_menu.js"
    },
    "host_permissions": [
        "*://*/*",
        "<all_urls>"
    ],
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content-scripts/confluence.js"],
        "run_at": "document_end"
    }, {
        "matches": ["<all_urls>"],
        "js": ["content-scripts/ant_designer.js"],
        "run_at": "document_end"
    }],
    "version": "0.0.5"
}
