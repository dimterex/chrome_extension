{
    "browser_action": {
        "default_icon": "icons/32.png",
        "default_title": "dimterex extension",
        "default_popup": "html/popup.html"
    },
    "content_security_policy": "script-src 'self'; object-src 'self'",
    "description": "Extension ",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "64": "icons/64.png",
        "128": "icons/128.png"
    },
    "manifest_version": 2,
    "name": "dimterex extension",
    "permissions": [
        "<all_urls>",
        "activeTab",
        "storage"
    ],
    "version": "0.0.1"
}
